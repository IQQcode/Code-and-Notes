方法覆写
//方法覆写不能出现private
class Person {
    public void fun() {
        this.print();
    }
     void print(){  
        System.out.println("1.Person类中的print方法！");
    }
}

class Student extends Person {
    public void print() {
        System.out.println("2.Student类中的print方法！");
    }
}

public class Override {
    public static void main(String[] args) {
        Student stu = new Student();
        stu.fun();
    }
}